<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/x8"
        android:layout_marginLeft="@dimen/x10"
        android:layout_marginRight="@dimen/x10"
        android:layout_marginTop="@dimen/x8"
        android:background="@drawable/bg_white_radius"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x8"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x15"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_item_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/x5"
                android:src="@drawable/item_icon_1" />

            <TextView
                android:id="@+id/tv_item_type"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/color_9"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/order_list_item_tv_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_3"
                android:textSize="13sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_item_desc1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x4"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x4"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_item_name1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_3"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tv_item_content1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/color_3"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tv_item_right1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x5"
                android:textColor="@color/color_3"
                android:textSize="13sp"
                android:visibility="gone" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_item_desc2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x4"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x4"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/tv_item_name2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_3"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tv_item_content2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/color_3"
                android:textSize="13sp" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_item_desc3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x4"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x4"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/tv_item_name3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_3"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/tv_item_content3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/color_3"
                android:textSize="13sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_item_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x4"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x4"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="商家信息："
                android:textColor="@color/color_3"
                android:textSize="13sp"
                android:visibility="invisible" />

            <LinearLayout
                android:id="@+id/ll_item_order_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" />

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x8"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x5">

            <TextView
                android:id="@+id/tv_item_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="0"
                android:textColor="@color/color_3"
                android:textSize="20sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/tv_item_price"
                android:layout_marginBottom="@dimen/x3"
                android:layout_toLeftOf="@+id/tv_item_price"
                android:text="总价："
                android:textColor="@color/color_3"
                android:textSize="12sp" />


        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#e0e0e0" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x8"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x8"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_item_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:textColor="@color/color_9"
                android:textSize="11sp" />

            <LinearLayout
                android:id="@+id/order_list_item_layout_evaluate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/to_invite_friend"
                    android:layout_width="@dimen/x100"
                    android:layout_height="@dimen/x30"
                    android:background="@drawable/shape_corner_pintuan"
                    android:gravity="center"
                    android:text="邀请好友参团"
                    android:textColor="@color/white"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/order_state_more_one"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/x30"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/x10"
                    android:background="@drawable/shap_gray_range_bg"
                    android:gravity="center"
                    android:minWidth="@dimen/x70"
                    android:text="@string/order_again"
                    android:textColor="@color/color_3"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <com.project.mgjandroid.ui.view.TimeTextView
                    android:id="@+id/order_state_go_pay"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/x30"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/x10"
                    android:background="@drawable/shap_pintuan_red_range_bg"
                    android:gravity="center"
                    android:minWidth="@dimen/x70"
                    android:text="@string/to_pay_order"
                    android:textColor="@color/pintuan_red"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/order_state_evaluate"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/x30"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/x10"
                    android:background="@drawable/shap_gray_range_bg"
                    android:gravity="center"
                    android:minWidth="@dimen/x70"
                    android:text="@string/evaluate_get_score"
                    android:textColor="@color/color_3"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/order_state_confirm"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/x30"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/x10"
                    android:background="@drawable/shap_pintuan_red_range_bg"
                    android:gravity="center"
                    android:minWidth="@dimen/x70"
                    android:text="@string/check_order_detail"
                    android:textColor="@color/pintuan_red"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/order_state_to_see_code"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/x30"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/x10"
                    android:background="@drawable/shap_gray_range_bg"
                    android:gravity="center"
                    android:minWidth="@dimen/x70"
                    android:text="查看券码"
                    android:textColor="@color/color_3"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/order_state_refund_to_balance"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/x30"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/x10"
                    android:background="@drawable/shap_gray_range_bg"
                    android:gravity="center"
                    android:minWidth="@dimen/x70"
                    android:text="退款详情"
                    android:textColor="@color/color_3"
                    android:textSize="14sp"
                    android:visibility="gone" />
            </LinearLayout>


        </LinearLayout>


    </LinearLayout>

